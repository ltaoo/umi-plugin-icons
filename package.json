    "@umijs/test-utils": "^1.0.0",
    "babel-plugin-import": "^1.13.3",
    "body-parser": "^1.18.2",
    "cross-env": "^6.0.3",
    "express": "^4.15.3",
    "father-build": "^1.17.2",
    "lint-staged": "^10.0.8",
    "npm-run-all": "^4.1.5",
    "pify": "^5.0.0",
    "prettier": "^1.19.1",
